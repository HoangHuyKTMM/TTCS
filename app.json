{
  "expo": {
    "name": "Reader_app",
    "slug": "Reader_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
       "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
     "scheme": "reader_app",
     "experiments": {
      "typedRoutes": true
     },
     "extra": {
       "googleExpoClientId": "",
       "googleAndroidClientId": "",
       "googleWebClientId": "",
       "facebookAppId": ""
     }

  }
}
