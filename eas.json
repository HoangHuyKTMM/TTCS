{
    "cli": {
        "version": ">= 5.0.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk",
                "gradleCommand": ":app:assembleDebug"
            },
            "env": {
                "EXPO_PUBLIC_API_BASE": "http://192.168.100.143:4000"
            }
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk",
                "gradleCommand": ":app:assembleRelease"
            },
            "env": {
                "EXPO_PUBLIC_API_BASE": "http://192.168.100.143:4000"
            }
        },
        "production": {
            "android": {
                "buildType": "app-bundle"
            }
        }
    },
    "submit": {
        "production": {}
    }
}